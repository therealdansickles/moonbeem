{
  "name": "server-restful",
  "version": "0.0.1",
  "description": "Vibe Restful Api",
  "author": "ritter@vibe.xyz",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "NODE_ENV='dev' nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --forceExit --logHeapUsage --runInBand",
    "test:watch": "jest --watch --forceExit --runInBand --logHeapUsage",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register $(yarn bin jest) --runInBand --logHeapUsage",
    "test:e2e": "jest --config ./test/jest-e2e.json --forceExit",
    "test:ci": "jest --ci --forceExit --runInBand --logHeapUsage",
    "typeorm:create": "ts-node --transpile-only ./node_modules/typeorm/cli.js --config src/lib/configs/postgres.config.ts migration:create --",
    "typeorm:generate": "npx ts-node ./node_modules/.bin/typeorm migration:generate -d src/lib/configs/postgres.config.ts migrations/migration",
    "typeorm:migrate": "npx ts-node --transpile-only ./node_modules/typeorm/cli.js migration:run -d src/lib/configs/postgres.config.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.319.0",
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@faker-js/faker": "^7.6.0",
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^2.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/typeorm": "^9.0.1",
    "@nextnm/nestjs-mailgun": "^3.0.0",
    "@openzeppelin/merkle-tree": "^1.0.4",
    "@sentry/node": "^7.47.0",
    "@uploadcare/upload-client": "^6.2.1",
    "apollo-server-core": "^3.12.0",
    "apollo-server-express": "^3.11.1",
    "aws-sdk": "^2.1322.0",
    "axios": "^1.3.5",
    "bcryptjs": "^2.4.3",
    "bignumber.js": "^9.1.1",
    "chalk": "^5.2.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commander": "^10.0.1",
    "dotenv": "^16.0.3",
    "ethers": "^6.3.0",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "lodash": "^4.17.21",
    "merkletreejs": "^0.3.10",
    "mongodb": "^4.13.0",
    "nest-raven": "^9.2.0",
    "nftscan-api": "^1.6.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.10.0",
    "randomstring": "^1.2.3",
    "redis": "^4.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.25.1",
    "typeorm": "^0.3.14",
    "uuid": "^9.0.0",
    "webpack": "^5.77.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/cron": "^2.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.34",
    "@types/randomstring": "^1.1.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.0.1",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "jest-util": "^29.5.0",
    "prettier": "^2.5.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.4.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@3.5.1"
}
